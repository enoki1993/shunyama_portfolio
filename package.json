{
  "name": "next-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "analyze-bundle": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{ts,tsx,json,css}'"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@primer/octicons-react": "^16.1.1",
    "microcms-js-sdk": "^1.2.0",
    "next": "^11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@next/bundle-analyzer": "^12.0.3",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.3.6",
    "prettier": "^2.0.2",
    "sass": "^1.39.0",
    "swr": "^0.5.6",
    "tailwindcss": "^2.2.15",
    "twin.macro": "^2.7.0",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
